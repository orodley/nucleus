(defun listn (n)
  (if (eq? n 0)
    nil
    (cons n (listn (- n 1)))))
 
(defun mas (x y z)
  (if (not (shorterp y x))
      z
      (mas (mas (cdr x) y z)
           (mas (cdr y) z x)
           (mas (cdr z) x y))))
 
(defun shorterp (x y)
  (and (not (nil? y))
       (or (nil? x)
           (shorterp (cdr x)
                     (cdr y)))))
 
(defun main ()
  (println (mas (listn 27) (listn 18) (listn 9))))
